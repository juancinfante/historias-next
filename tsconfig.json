{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/components/*": ["./app/components/*"]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/dashboard/reservas/columns.jsx", "src/app/dashboard/reservas/page.jsx", "src/app/dashboard/reservas/data-table.jsx", "src/app/dashboard/viajes/[id]/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
